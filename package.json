{
  "name": "avrit-ui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@textile/hub": "^1.0.0",
    "axios": "^0.19.2",
    "formik": "^2.1.5",
